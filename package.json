{
  "name": "reactjstask",
  "version": "1.0.0",
  "description": "ReactJS App",
  "entry point": "server.js",
  "main": "index.js",
  "scripts": {
    "start": "babel-node buildTools/server.js",
    "open:src": "webpack-dev-server --hot --port 4000",
    "build:dev": "webpack --env dev --progress --profile --colors",
    "build:prod": "webpack --env prod --progress --profile --colors",
    "test": "jest",
    "test:watch": "jest --watchAll",
    "test:coverage": "npm run test -- --coverage",
    "test:open": "cypress open"
  },
  "jest": {
    "setupFiles": [
      "./jest/enzyme.js"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ketanthakkar/ReactMentoring.git"
  },
  "author": "Ketan Thakkar",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ketanthakkar/ReactMentoring/issues"
  },
  "homepage": "https://github.com/ketanthakkar/ReactMentoring#readme",
  "dependencies": {
    "express": "^4.16.4",
    "react": "^16.6.3",
    "react-dom": "^16.6.3",
    "react-redux": "^5.1.1",
    "react-router-dom": "^4.3.1",
    "redux": "^4.0.1",
    "redux-thunk": "^2.3.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.4",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "css-loader": "^1.0.1",
    "cypress": "^3.1.3",
    "enzyme": "^3.8.0",
    "enzyme-adapter-react-16": "^1.7.1",
    "enzyme-to-json": "^3.3.5",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "fetch-mock": "^7.2.5",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^23.6.0",
    "jest-fetch-mock": "^2.0.1",
    "json-loader": "^0.5.7",
    "mini-css-extract-plugin": "^0.4.5",
    "npm-run-all": "^4.1.5",
    "open": "0.0.5",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "redux-devtools": "^3.4.2",
    "redux-mock-store": "^1.5.3",
    "style-loader": "^0.23.1",
    "uglifyjs-webpack-plugin": "^2.0.1",
    "webpack": "^4.26.1",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.14"
  },
  "babel": {
    "presets": [
      "env",
      "react",
      "stage-2"
    ],
    "plugins": [
      [
        "transform-runtime",
        {
          "regenerator": true
        }
      ]
    ]
  }
}
